            <!--Region BasePickerPage-->
            <Style x:Key="BasePickerPageStyle" TargetType="controls:BasePickerPage">
                <Setter Property="SupportedOrientations" Value="Portrait" />
                <Setter Property="Orientation" Value="Portrait" />
                <Setter Property="toolkit:TiltEffect.IsTiltEnabled" Value="True" />
                <Setter Property="shell:SystemTray.IsVisible" Value="True" />
                <Setter Property="shell:SystemTray.ForegroundColor" Value="{StaticResource BasePickerPageForegroundColor}" />
                <Setter Property="shell:SystemTray.BackgroundColor" Value="{StaticResource BasePickerPageBackgroundColor}" />
            </Style>

            <Style x:Key="BasePickerPageRootStyle" TargetType="Grid">
                <Setter Property="Background" Value="{StaticResource BasePickerPageBackgroundBrush}" />
            </Style>

            <Style x:Key="BasePickerPageHeaderStyle" TargetType="TextBlock">
                <Setter Property="IsHitTestVisible" Value="False" />
                <Setter Property="FontFamily" Value="Segoe WP Semibold" />
                <Setter Property="FontSize" Value="22.667" />
                <Setter Property="Foreground" Value="{StaticResource BasePickerPageForegroundBrush}" />
                <Setter Property="Margin" Value="24,16,24,24" />
            </Style>

            <Style x:Key="BasePickerQuickButtonTextStyle" TargetType="TextBlock" BasedOn="{StaticResource ButtonTextStyle}">
                <Setter Property="FontSize" Value="20" />
            </Style>

            <Style x:Key="BasePickerQuickButtonStyle" TargetType="Button">
                <Setter Property="BorderThickness" Value="1" />
                <Setter Property="BorderBrush" Value="{StaticResource ButtonBackgroundBrush}" />
                <Setter Property="Foreground" Value="{StaticResource ButtonForegroundBrush}" />
                <Setter Property="Margin" Value="0,-5" />
                <Setter Property="Padding" Value="0" />
                <Setter Property="Height" Value="60" />
                <Setter Property="ContentTemplate">
                    <Setter.Value>
                        <DataTemplate>
                            <TextBlock Text="{Binding}" Style="{StaticResource BasePickerQuickButtonTextStyle}" />
                        </DataTemplate>
                    </Setter.Value>
                </Setter>
